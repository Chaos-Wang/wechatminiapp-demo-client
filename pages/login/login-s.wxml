<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
    <view class="page__hd">
        <view class="page__title">实名信息完善</view>
        <view class="page__desc">用于相关功能</view>
    </view>
    <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">身份证号</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入身份证号码"bindinput="IDInput"/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">真实姓名</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入真实姓名" bindinput="NameInput"/>
                </view>          
            </view>
        </view>


        <view class="weui-btn-area">
            <button class="weui-btn" type="primary" bindtap="fill">确定</button>
        </view>
    </view>